{
    "$schema": "file:///F:/web/MerapiPanel/schema/block.json",
    "name": "core/group",
    "title": "Group",
    "attributes": [
        {
            "name": "title",
            "type": "string",
            "default": "Hallo World"
        },
        {
            "name": "length",
            "type": "number",
            "default": 0
        },
        {
            "name": "withoutTitle",
            "type": "boolean",
            "default": false
        },
        {
            "name": "colors",
            "type": "array",
            "default": [
                "red",
                "green",
                "blue"
            ]
        }
    ],
    "options": {
        "droppable": true,
        "children": [
            "core/paragraph",
            "core/heading"
        ]
    },
    "editScript": "file:./edit.js",
    "editStyle": "file:./edit.scss",
    "save": "file:./save.js",
    "style": "file:./style.scss"    
}